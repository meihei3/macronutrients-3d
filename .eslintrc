{
    "env": {
        "browser": true,
        "es6": true
    },
    "globals": {
        "Plotly": false
    },
    "extends": [
        "eslint:recommended",
        "plugin:prettier/recommended"
    ],
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "singleQuote": true,
                "trailingComma": "es5"
            }
        ]
    },
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 8
    }
}